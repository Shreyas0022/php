CREATE TABLE `studentregistration`.`students` (`id` INT NOT NULL AUTO_INCREMENT , `first_name` VARCHAR(30) NOT NULL , `last_name` VARCHAR(30) NOT NULL , `dob` DATE NOT NULL , `email` VARCHAR NOT NULL , `mobile` INT(10) NOT NULL , `password` VARCHAR(30) NOT NULL , PRIMARY KEY (`id`), UNIQUE (`email`)) ENGINE = InnoDB; 
